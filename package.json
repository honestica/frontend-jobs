{
  "name": "lifen-frontend-jobs",
  "version": "0.1.0",
  "private": false,
  "engines": {
    "node": ">=14.16.0"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start:electron": "wait-on http://localhost:3000 && electron .",
    "start:server": "BROWSER=none yarn react-scripts start",
