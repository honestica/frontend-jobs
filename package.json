  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "eslint-utils": "^1.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "electron": "19.0.9",
    "wait-on": "^3.3.0"
  }
}
